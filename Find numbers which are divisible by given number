using System;
using System.Collections;
using System.Collections.Generic;
using System.Linq;


public class Kata
{
  public static int[] DivisibleBy(int[] numbers, int divisor)
  {
    
    List<int> divisiblenumbers = new List<int>();
    
    
    for(int i = 0; i < numbers.Length; i++)
    {
      if(numbers[i] % divisor == 0)
      {
        int divisiblenumber = numbers[i];
        divisiblenumbers.Add(divisiblenumber);
      }
    }
    
    int[] divisiblenumbersarray = divisiblenumbers.ToArray();
    return divisiblenumbersarray;
  }
}
